{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env MODE=development vite",
    "dev:all": "concurrently \"cross-env MODE=development vite\" \"cd ../backend && npm run dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "build:dev": "cross-env MODE=development vite build",
    "build:prod": "cross-env MODE=production vite build",
    "test": "jest test.js --roots src",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "vitest",
    "test:unit:react": "jest test.ts --roots src",
    "test:integration": "jest test.ts --roots tests/integration",
    "test:e2e": "jest test.ts --roots tests/e2e",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "visual:report": "npm-run-all --serial visual:report:*",
    "visual:report:json": "node scripts/generate-visual-json-report.ts",
    "visual:report:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@cyntler/react-doc-viewer": "^1.17.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/antd": "^1.0.0",
    "@types/lodash": "^4.14.196",
    "@types/react": "^19.0.10",
    "antd": "^5.8.2",
    "async-mutex": "^0.4.1",
    "axios": "^1.7.4",
    "classnames": "^2.5.1",
    "config": "^3.3.9",
    "framer-motion": "^10.12.9",
    "i18next": "^24.2.1",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.1",
    "is-image-url": "^1.1.8",
    "lodash-es": "^4.17.21",
    "proptypes": "^1.1.0",
    "react": "^19.0.0",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.0",
    "react-player": "^2.12.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-selecto": "^1.22.5",
    "react-tsparticles": "^2.12.2",
    "redux-persist": "^6.0.0",
    "sass": "^1.69.7",
    "ts-node": "^10.9.2",
    "tsparticles": "^2.9.3",
    "use-sound": "^4.0.1",
    "util": "^0.12.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.18.0",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-onboarding": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/react-vite": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-i18next": "^6.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.11.2",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.3",
    "loki": "^0.35.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "react-responsive": "^9.0.2",
    "reg-cli": "^0.18.10",
    "regenerator-runtime": "^0.14.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "storybook": "^8.5.3",
    "stylelint": "^16.13.2",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^4.5.9",
    "vitest": "^3.0.6"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  }
}
